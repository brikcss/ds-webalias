{
  "name": "@brikcss/webalias",
  "version": "0.1.0",
  "description": "A DirectScale, inc. module which turns any site into a replicated site, using the DirectScale webalias.",
  "dependencies": {
    "@brikcss/element": "^0.1.3",
    "hyperhtml": "^2.25.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.3.4",
    "@brikcss/git-hooks": "^2.1.2",
    "@brikcss/merge": "^1.0.7",
    "browser-sync": "^2.26.3",
    "codacy-coverage": "^3.4.0",
    "coveralls": "^3.0.3",
    "document-register-element": "^1.13.1",
    "jest": "^24.5.0",
    "rollup": "^1.6.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-terser": "^4.0.4"
  },
  "scripts": {
    "start": "npm run watch",
    "pretest": "npm run build",
    "test": "npx standard && jest --coverage",
    "cov": "cat coverage/lcov.info | coveralls && cat coverage/lcov.info | codacy-coverage",
    "build": "npx rollup --config=.rolluprc.js",
    "watch": "npm run build -- --watch & npm run serve",
    "serve": "npx browser-sync start --config .browsersyncrc.js"
  },
  "files": ["dist", "examples"],
  "main": "dist/esm/webalias.js",
  "module": "dist/esm/webalias.js",
  "browser": "dist/umd/webalias.js",
  "keywords": ["brikcss"],
  "author": "Brikcss (https://github.com/brikcss)",
  "license": "MIT",
  "publishConfig": {
    "tag": "dev",
    "access": "public"
  }
}
